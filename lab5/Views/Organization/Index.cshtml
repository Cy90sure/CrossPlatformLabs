@model List<lab5.Models.Organization>

@{
    ViewData["Title"] = "Organizations";
}

<h1>Organizations</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Organization ID</th>
            <th>Type Code</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var organization in Model)
        {
            <tr>
                <td>@organization.OrganizationId</td>
                <td>@organization.OrganizationTypeCode</td>
                <td>@organization.OrganizationDetails</td>
            </tr>
        }
    </tbody>
</table>
